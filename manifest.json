{
    "manifest_version": 3,
    "name": "Time Tracker",
    "version": "1.0",
    "permissions": [
        "tabs",
        "storage",
        "webNavigation",
        "activeTab"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html",
      "default_title": "Time Tracker"
    },
    "host_permissions": ["<all_urls>"]
  }
  